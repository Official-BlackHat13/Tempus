                    //AB-StoreTopStart
                    on_key kHelp send DisplayHelpFile
                    //AB-StoreTopEnd

                    // Order Maintenance
                    #IFDEF CE_PRODUCTION
                    Entry_Item Esjitm.Instruction
                    #ENDIF // #IFDEF CE_PRODUCTION
                    // Estimating
                    #IFDEF CE_CALC_ESTIMATE
                    Entry_Item Esitem.Instruction
                    #ENDIF // #IFDEF CE_CALC_ESTIMATE
                    // component library
                    #IFDEF CE_CALC_LIBRARY
                    Entry_Item Eslitm.Instruction
                    #ENDIF // #IFDEF CE_CALC_LIBRARY

                    Set Size to 34 344
                    Set Location to 10 5

                    //AB-StoreStart
                    set Shadow_State to true
                    //AB-StoreEnd

                End_Object    // Esitem_Instruction
                procedure EnableInstruction integer bState
                    set Enabled_State of Item_Instruction to bState
                end_procedure

            End_Object    // InstructionGroup

        End_Object    // TP_Instruction

        Procedure Request_Switch_To_Tab integer iPage integer iMode
            if (pbEditing(self) AND iPage >= 1) send Activate of EditButton
            forward send Request_Switch_To_Tab iPage iMode
        End_Procedure

    End_Object    // ItemTabDialog

    Object EditButton is a cGlblButton

        //AB-StoreTopStart
        property boolean bSaveButton FALSE
        //AB-StoreTopEnd

        Set Label to "&Edit"
        Set Location to 65 235 //215

        //AB-StoreStart
        procedure OnClick
            if (bSaveButton(self)) begin
                set pbSaving to TRUE
                send Request_Save
            end
            else begin
                send DoEdit
                send Activate of ItemTabDialog
            end
        end_procedure
        procedure EnableSaveButton boolean bState
            if (bState) set Label to "&Save"
            else        set Label to "&Edit"
            set bSaveButton to bState
        end_procedure
        //AB-StoreEnd

    End_Object    // EditButton

    Object HelpButton is a cGlblButton

        //AB-StoreTopStart

        //AB-StoreTopEnd

        Set Label to "&Help"
        Set Location to 65 290 //270

        //AB-StoreStart
        procedure OnClick
            send DisplayHelpFile
        end_procedure
        //AB-StoreEnd

    End_Object    // HelpButton

    Object CloseButton is a cGlblButton
        Set Label to "&Close"
        Set Location to 65 345 //325

        //AB-StoreStart
        procedure OnClick
            send Close_Panel
        end_procedure
        //AB-StoreEnd

    End_Object    // CloseButton


    //AB-StoreStart
    procedure DoEdit
        integer hDD
        string  sNeedsCalced
        //
        get Server to hDD
        // Order Maintenance
        #IFDEF CE_PRODUCTION
        get Field_Current_Value of hDD field Esjcmp.Needs_calced to sNeedsCalced
        #ENDIF // #IFDEF CE_PRODUCTION
        // Estimating
        #IFDEF CE_CALC_ESTIMATE
        get Field_Current_Value of hDD field Escomp.Needs_calced to sNeedsCalced
        #ENDIF // #IFDEF CE_CALC_ESTIMATE
        // component library
        #IFDEF CE_CALC_LIBRARY
        get Field_Current_Value of hDD field Esclib.Needs_calced to sNeedsCalced
        #ENDIF // #IFDEF CE_CALC_LIBRARY
        //
        set pbNeedsCalced to (sNeedsCalced = "Y")
        set pbEditing     to TRUE
        set pbSaving      to FALSE
        set pbClearing    to FALSE
        set pbDupeValue   to FALSE
        set pnDupeValue   to 0
        //
        send EnableSaveButton  of EditButton TRUE
        send EditVariables     of (VariableGroup(TP_Formula_Variables(ItemTabDialog(self))))
        send EnableInstruction of (InstructionGroup(TP_Instruction(ItemTabDialog(self)))) TRUE
    end_procedure

    procedure DisplayHelpFile
        string sStandard
        //
        send Refind_Records of (Server(self))
        move Jcoper.Opcode to sStandard
        //
        append sStandard " " Jcoper.Name
        // show it
        send RetrieveHelp of (OpcodeHelpDialog(self)) Jcoper.Cstctr_idno Jcoper.Opcode sStandard
    end_procedure

    Procedure Close_Panel
        Send ShadowVariables  of (VariableGroup(TP_Formula_Variables(ItemTabDialog(Self))))
        Send EnableSaveButton of  EditButton False
        Forward Send Close_Panel
    End_Procedure

    procedure Request_Clear
        send Close_Panel
    end_procedure

    procedure Request_Clear_All
        Send Close_Panel
    end_procedure

    //procedure ExitDetailPanel
    //    send Request_Cancel
    //end_procedure

    procedure Request_Save
        forward send Request_Save
        if (Should_Save(self)) procedure_return
        //
        set pbEditing to FALSE
        //
        send ShadowVariables       of (VariableGroup(TP_Formula_Variables(ItemTabDialog(self))))
        send EnableInstruction     of (InstructionGroup(TP_Instruction(ItemTabDialog(self)))) FALSE
        send EnableSaveButton      of EditButton FALSE
        send Request_Switch_To_Tab of ItemTabDialog 0 3
        //send Activate to (ItemTabDialog(self))
        send Close_Panel
    end_procedure

    procedure EditItemDirect integer iItemRec
        set pbEditDirect to TRUE
        send DoDisplayVariables iItemRec FALSE
    end_procedure

    procedure DoDisplayVariables integer iItemRec boolean bLocked
        integer hDD iOpcode bState iFile iField
        string  sLabel sCenter sDescription sCalcflags
        //
        set pbEditing     to FALSE
        set pbNeedsCalced to FALSE
        set pbSaving      to FALSE
        set pbClearing    to FALSE
        //
        set Enabled_State of EditButton to (NOT(bLocked))
        //
        get piItemFile    to iFile
        get piOpcodeField to iField
        //
        get Server          to hDD
        send Clear          of hDD
        send Find_By_Recnum of hDD iFile iItemRec
        //
        get Field_Current_Value of hDD iField to iOpcode
        //
        get File_Field_Current_Value of hDD file_field Jccntr.Nickname to sCenter
        get File_Field_Current_Value of hDD file_field Jcoper.Name     to sDescription
        move (string(iOpcode) * "-" * sCenter * "-" * sDescription)    to sLabel
        set Label                                                      to sLabel
        //
        send DoSetVariableControls of (VariableGroup(TP_Formula_Variables(ItemTabDialog(self))))
        //
        if (pbEditDirect(self)) begin
            send DoEdit
            set pbEditDirect to FALSE
        end
        //
        send Popup_Modal
    end_procedure // DoDisplayVariables

    on_key Key_Alt+Key_E send KeyAction of EditButton
    on_key Key_Alt+Key_S send KeyAction of EditButton
    on_key Key_Alt+Key_H send KeyAction of HelpButton
    on_key Key_Alt+Key_C send KeyAction of CloseButton
    //AB-StoreEnd

End_Object    // oVariablesDialog

