    procedure DoLoadStandardsArray
        integer bFound iRowCount iIndex hStatusPanel
        number  nTotal nCurrent nPercent
        string  sSentinel
        //
        send Cursor_Wait of Cursor_Control
        //
        get_attribute DF_FILE_RECORDS_USED of Jcoper.File_number to nTotal
        //
        send Delete_Data
        //
        move (Status_Panel(self)) to hStatusPanel
        if (hStatusPanel) begin
            //Get Sentinel_Name of hStatusPanel to sSentinel
            //Set Sentinel_Name of hStatusPanel to "prgssent"
            send Initialize_StatusPanel of hStatusPanel ;
                "Caching standards" "Please wait..." ""
            set Allow_Cancel_State of hStatusPanel to FALSE
            send Start_StatusPanel of hStatusPanel
        end // if (hStatusPanel)
        //
        clear Jcoper
        find GE Jcoper.Opcode
        move (FOUND) to bFound
        while (bFound)
            relate Jcoper
            move (nCurrent + 1) to nCurrent
            //
            if (Jcoper.Est_std = "Y") begin
                set Array_Value item iIndex to Jcoper.Opcode
                increment iIndex
                set Array_Value item iIndex to (trim(Jcoper.Name))
