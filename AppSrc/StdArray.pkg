    //
    function IsOpcodeRow integer iOpcode returns integer
        integer iRowCount iRow iIndex iValue
        get piRowCount to iRowCount
        while (iRow < iRowCount)
            get Integer_Value item (iRow * 179) to iValue
            if (iValue = iOpcode) break
            increment iRow
        loop
        function_return iRow
    end_function
