            End
            If (iToQuotehdrContactidno<>0) Begin
                Move (sSelection+(If(sSelection<>""," and ",""))+"{QUOTEHDR.ContactIdno} <= "+(String(iToQuotehdrContactidno))) to sSelection
            End
            
            
